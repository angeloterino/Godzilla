@model StrawmanApp.Models.StrawmanViewModel

@{    
    string[] _colData = (string[])ViewData["_colData"];
    string[] _colType = (string[])ViewData["_colType"];
    StrawmanApp.Models.MarketViewChannelModels itemaux = Model.itemaux;
    StrawmanApp.Models.MarketViewChannelModels item = Model.item;
    string[] _colPureData = new string[] { item.col1.ToString(), item.col2.ToString(), item.col3.ToString(), item.col4.ToString(), item.col5.ToString(), item.col6.ToString() };
    string _style = "text-align:center;";
    string _styleView = "_TrStyleFranchise";
    ViewDataDictionary _styleData = new ViewDataDictionary() { { "_style", _style }, { "has_child", itemaux.vhas_child }, { "id", itemaux.vid }, { "parent", itemaux.vparent }, { "order", itemaux.vorder } };
    StrawmanApp.Models.StrwawmanTrViewModel model = new StrawmanApp.Models.StrwawmanTrViewModel{
        _colPureData = _colPureData,
        _colData = _colData,
        _colType = _colType,
        styleView = _styleView,
        styleData = _styleData,
        _colAttr = Model.item != null && Model.item.col2_wc != null ? new string[] 
                                                                                { 
                                                                                    _colType[0] != null && _colType[0] == "pc" ? "data-wc=\"" + Html.DisplayFor(m=>Model.item.col1_wc).ToString() + "\"" : "data-wc=\"" + Model.item.col1_wc + "\"", 
                                                                                    _colType[1] != null && _colType[1] == "pc" ? "data-wc=\"" + Html.DisplayFor(m => Model.item.col2_wc).ToString() + "\"" : "data-wc=\"" + Model.item.col2_wc + "\"",
                                                                                    _colType[2] != null && _colType[2] == "pc" ? "data-wc=\"" + Html.DisplayFor(m => Model.item.pcvspy_wc).ToString() + "\"" : "data-wc=\"" + Model.item.pcvspy_wc + "\"" 
                                                                                } : null
        };
}    
@{Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_TrView.cshtml", model);}
