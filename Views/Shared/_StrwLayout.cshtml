@{
    if (StrawmanApp.Helpers.UserUtils.UserName == null) { Response.Redirect("~/Account/LogIn?returnUrl=" + ViewBag.Url); }
    string title_ = ViewBag.Title;
    string _style = "";
    if (ViewBag.Title == "BoyMassMarket")
        {
            ViewData["channel"] = "MASS";
        }
        else if (ViewBag.Title == "BoyBeauty")
        {
            ViewData["channel"] = "BEAUTY";
        }
        else if (ViewBag.Title == "BoyOTC")
        {
            ViewData["channel"] = "OTC";
        }
}
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=9" />
        <title>@ViewBag.Title - Insight Application</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" />
        <link href="@Url.Content("~/Content/yeti/bootstrap.min.css")" rel="stylesheet" />
        <script src="@Url.Content("~/Scripts/jquery-2.1.4.min.js")"></script>
        <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
        <script src="@Url.Content("~/Scripts/gridmvc.min.js")"></script>
        <script src="@Url.Content("~/Scripts/common/functions.js")"></script>
        @Styles.Render("~/bundles/yeti")
        @{Html.RenderPartial("~/Views/Shared/_Scripts.cshtml", new ViewDataDictionary{ {"menuUrl", ViewBag.Title} });}
        @Styles.Render("~/Content/css")
        @Styles.Render("~/Content/themes/base/css")
        @Styles.Render("~/bundles/fontawesome")
        @Scripts.Render("~/bundles/modernizr")
        @Styles.Render("~/bundles/jansybootstrap/css") 
        @Scripts.Render("~/jansybootstrap/jquery")
        @if (ViewBag.Title == "StrawmanApp") { _style = "min-width:3000px;"; }
    </head>
    <body>
        <div id="body" style="">
                <div style="" class="menu_wrapper">                
                <div class="float-left">
                @{ Html.RenderAction("Tabs", "Tabs", new { title = ViewBag.Title, menu_url = ViewBag.MenuUrl == null ? ViewBag.Title : ViewBag.MenuUrl, tab_url = ViewBag.TabUrl });}           
                </div>
                <div class="menu wrapper form-group" style="float:right;">
                @{ Html.RenderAction("Menu", "Menu");}
                </div>
                @{ Html.RenderAction("Banner", "Menu", new { title = ViewBag.Title, menu_url = ViewBag.MenuUrl == null ? ViewBag.Title : ViewBag.MenuUrl, tab_url = ViewBag.TabUrl });}
                </div>
            <div style="top: 50px; width: 100%; bottom: 0px; position: absolute; background-color: rgb(255, 255, 255);">
            <div style="width: 100%; height: 100%; overflow: auto;" id="wrapper_main"">
            <div style="clear:both;"></div>
            <div style="@_style">
                @RenderBody()                
            </div>
            </div>
            </div>            
        </div>
        <input type="hidden" id="cache_set" value="true" />
    </body>
</html>
