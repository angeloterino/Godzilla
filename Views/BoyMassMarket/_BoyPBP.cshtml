@{
  int vb_index = 0;

  List<StrawmanApp.Models.BoyMassMarketModels> lstaux = (List<StrawmanApp.Models.BoyMassMarketModels>)ViewBag.BoyCALC;
  int _year = (ViewBag.YearPeriod == null) ? 2015 : int.Parse(new DateTime((int)ViewBag.YearPeriod, 1, 1).ToString("yyyy"));
  int _syear = (ViewBag.YearPeriod == null) ? 15 : int.Parse(new DateTime((int)ViewBag.YearPeriod, 1, 1).ToString("yy"));
  foreach (StrawmanApp.Models.BoyMassMarketModels yitemaux in Model)
    {
        
        StrawmanApp.Models.BoyMassMarketModels item = lstaux.Find(m=>m.brand == yitemaux.brand && m.market==yitemaux.market);
        Html.RenderPartial("~/Views/BoyMassMarket/_BoyStdCalcView.cshtml", item, new ViewDataDictionary { { "yitemaux", yitemaux }, { "header", new[] { (_year + 1).ToString() + " PBP", "% Growth VYA" } } });  
        vb_index++;
    }       
}
